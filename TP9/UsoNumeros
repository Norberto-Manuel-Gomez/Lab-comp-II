package TP9;

public class UsoNumeros {

    public static int sumaNaturales (Numero numeros[]) {

        int resultado = 0;

        for (int i = 0; i < numeros.length; i++) {

            if (numeros[i].getTipo().equals("Natural")) {
                resultado += numeros[i].getValor();
            }
        }

        return resultado;
    }

}
